   ],
   "source": [
    "number = int(input(\"Enter a number : \"))\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(1, number):\n",
    "    if number % i == 0:\n",
    "        count += 1\n",
    "    else:\n",
    "        count += 0\n",
    "\n",
    "if count == 1:\n",
    "    print(\"{} is a prime number\".format(number))\n",
    "else:\n",
    "    print(\"{} is not a prime number\".format(number))"
   ]
